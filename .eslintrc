{
  "env": {
    "browser": true,
    "node": true,
  },
  "extends": "airbnb",
  "parser": "babel-eslint",
  "rules": {
    "import/no-extraneous-dependencies": [false],
    "jsx-a11y/anchor-is-valid": [ "error", {
      "components": [ "Link" ],
      "specialLink": [ "to", "hrefLeft", "hrefRight" ],
      "aspects": [ "noHref", "invalidHref", "preferButton" ],
    }],
    "import/exports-last": [ "error" ],
  },
  "plugins": [
    "react",
  ]
}
