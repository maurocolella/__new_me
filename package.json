{
  "name": "mauro-colella-portfolio",
  "version": "1.0.4",
  "description": "Portfolio based on ReactJS hot code reloading boilerplate",
  "scripts": {
    "start": "nodemon index.js --exec babel-node",
    "start:dev": "NODE_ENV=development npm start",
    "test": "jest --coverage",
    "test:bench": "node utils/lighthouse-runner.js",
    "test:ci": "npm run lint && jest -w 1 --coverage && npm run build && npm run uml:generate",
    "uml:generate": "node utils/plantuml-runner.js",
    "lint": "node node_modules/eslint/bin/eslint src --ext .jsx --ext .js src/",
    "build": "rimraf dist/* && webpack"
  },
  "jest": {
    "moduleNameMapper": {
      "\\.(css|less|scss|sss|styl)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!**/node_modules/**",
      "!**/vendor/**"
    ],
    "coverageDirectory": "reports/coverage"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/maurocolella/__new_me.git"
  },
  "keywords": [
    "react",
    "reactjs",
    "boilerplate",
    "hot",
    "reload",
    "hmr",
    "live",
    "edit",
    "webpack"
  ],
  "author": "Mauro Colella <mauro.ludovico.colella@gmail.com> (http://github.com/maurocolella)",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/maurocolella/__new_me/issues"
  },
  "homepage": "https://github.com/maurocolella/__new_me",
  "devDependencies": {
    "@babel/cli": "^7.8.4",
    "@babel/core": "^7.9.0",
    "@babel/node": "^7.8.7",
    "@babel/plugin-proposal-class-properties": "^7.8.3",
    "@babel/plugin-syntax-dynamic-import": "^7.8.3",
    "@babel/plugin-transform-runtime": "^7.9.0",
    "@babel/preset-env": "^7.9.0",
    "@babel/preset-react": "^7.9.4",
    "babel-eslint": "^10.1.0",
    "babel-jest": "^24.8.0",
    "babel-loader": "^8.1.0",
    "chrome-launcher": "^0.10.7",
    "css-loader": "^3.4.2",
    "eslint": "^6.8.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.20.1",
    "eslint-plugin-jsx-a11y": "^6.2.3",
    "eslint-plugin-react": "^7.19.0",
    "express": "^4.17.1",
    "identity-obj-proxy": "^3.0.0",
    "isomorphic-webpack": "^2.1.1",
    "jest": "^24.8.0",
    "lighthouse": "^5.6.0",
    "mkdirp": "^0.5.1",
    "node-plantuml": "^0.8.1",
    "node-sass": "^4.13.1",
    "node-style-loader": "^0.0.1-alpha",
    "nodemon": "^1.19.1",
    "react-test-renderer": "^16.13.1",
    "responsive-loader": "^1.2.0",
    "rimraf": "^2.6.3",
    "sass-loader": "^7.1.0",
    "sharp": "^0.22.1",
    "style-loader": "^0.23.1",
    "uglifyjs-webpack-plugin": "^2.2.0",
    "url-loader": "^2.0.1",
    "webpack": "^4.42.1",
    "webpack-bundle-analyzer": "^3.6.1",
    "webpack-cli": "^3.3.11",
    "webpack-dev-middleware": "^3.7.2",
    "webpack-hot-middleware": "^2.25.0"
  },
  "dependencies": {
    "@babel/polyfill": "^7.8.7",
    "body-parser": "^1.19.0",
    "camelize": "^1.0.0",
    "cookie-parser": "^1.4.5",
    "dotenv": "^8.2.0",
    "ejs": "^2.6.2",
    "express-session": "^1.17.0",
    "gsap": "^2.1.3",
    "history": "^4.10.1",
    "isomorphic-fetch": "^2.2.1",
    "mobile-detect": "^1.4.4",
    "moment": "^2.24.0",
    "normalize.css": "^8.0.1",
    "prop-types": "^15.7.2",
    "react": "^16.13.1",
    "react-dom": "^16.13.1",
    "react-ga": "^2.7.0",
    "react-hot-loader": "^4.12.20",
    "react-particles-js": "^2.7.1",
    "react-redux": "^7.2.0",
    "react-reveal": "^1.2.2",
    "react-router-dom": "^5.1.2",
    "react-router-prop-types": "^1.0.4",
    "redux": "^4.0.5",
    "redux-thunk": "^2.3.0",
    "session-storage-cache": "^1.0.4",
    "smoothscroll-polyfill": "^0.4.4",
    "three": "^0.106.0"
  }
}
